<?xml version="1.0" encoding="utf-8" ?>
<rosbridge_config>
  <network ipaddress="10.2.94.180" port="9090" protocol="ws"/>
  <subscriptions>
    <topic name="/base_scan"/>
    <topic name="/turtle1/pose"/>
    <topic name="/base_odometry/odometer"/>
  </subscriptions>
  <publications>
    <topic name="/turtle1/cmd_vel" target="turtle"/>
  </publications>
  <projections>
    <query topic="/turtle1/pose" attribute="x"/>
    <query topic="/turtle1/pose" attribute="y"/>
    <query topic="/scan_front" attribute="ranges"/>
  </projections>
  <visualization>
    <scale r="10.0"/>
    <laser_field topic="/base_scan"/>
    <odometry topic="/base_odometry/odometer"/>
    <showState topic="/DriveStates"/>
  </visualization>
</rosbridge_config>